#!/usr/bin/env escript
%% -*- erlang -*-
%%! -pa ebin

main(_) ->
    ok = application:start(pubsubpoll),
    {ok, _ChanA} = pubsubpoll:create_channel({}),
    {ok, _ChanB} = pubsubpoll:create_channel({}),
    io:format("Chan: ~p~n", [pubsubpoll:channels()]),
    ok = pubsubpoll:publish([{name, "Robert"}]).
